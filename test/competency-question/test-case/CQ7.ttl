	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"length\" ] } ,  \"results\": {  \"bindings\": [ {  \"length\": {  \"datatype\":  \"http://www.w3.org/2001/XMLSchema#double\" , \"type\":  \"literal\", \"value\": \"3.42\" } } ] } }";
